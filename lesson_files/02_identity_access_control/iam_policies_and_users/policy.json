{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListObjectsInBucket",
            "Effect": "Allow",
            "Action": ["s3:ListBucket"],
            "Resource": [
				"arn:aws:s3:::ac-catpics-XXX",
				"arn:aws:s3:::ac-dogpics-XXX"
			]
        },
        {
            "Sid": "AllObjectActions",
            "Effect": "Allow",
            "Action": "s3:*Object",
            "Resource": ["arn:aws:s3:::ac-catpics-XXX/*"]
        }
    ]
}